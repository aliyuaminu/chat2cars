<script>
	$(document).ready(function() {
        let nav = $('.navbar-default .sidebar-collapse #side-menu .nav-btn');
        let container = $('#append-data');
        let origin = window.location.origin;
        let href = '/search/';
		$('#top-search').on('keyup change', function() {
            let keywords = $.trim($(this).val());
            let urlencode = keywords.split(' ').join('+');
            let path = keywords.length > 0 ? origin+href+urlencode : origin;
            nav.removeClass('active');
            window.document.title = keywords.length == 0 ? 'JIBWIS - Nigeria' : 'Search for: '+keywords;
            window.history.pushState({}, null, path);
            $.get(path, res => {
                container.hide().html(res).fadeIn('normal');
            });
		});
	});
</script>

